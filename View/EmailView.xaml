      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="546"
	Title="EmailView">

    <Grid>
        <GroupBox Header="SMTPs" Height="263" HorizontalAlignment="Left" Margin="20,23,0,0" Name="groupBox1" VerticalAlignment="Top" Width="440">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="18*" />
                    <RowDefinition Height="222*" />
                </Grid.RowDefinitions>
                <StackPanel Height="28" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Width="134" Grid.RowSpan="2">
                    <Label Content="Reply address:" Height="26" Name="label1" Width="136" />
                </StackPanel>
                <StackPanel Name="stackPanel2" Width="132" Margin="142,0,0,212" HorizontalAlignment="Left" Grid.RowSpan="2">
                    <TextBlock Height="28" Name="textBlock1" Text="TextBlock" Width="123" Padding=" 0,5,0,0" />
                </StackPanel>
            </Grid>
        </GroupBox>
    </Grid>
</Window>
